<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.ifam.saf">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MainApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="SAF"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".criaritem.CriarItemActivity"
            android:label="Criar Item" />
        <activity
            android:name=".requisitarreserva.ReservaActivity"
            android:label="@string/title_activity_reserva"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".criarconta.CriarContaActivity"
            android:label="SAF - Criar conta" />
        <activity android:name=".login.LoginActivity" />
        <activity
            android:name=".editarconta.EditarContaActivity"
            android:label="SAF - Editar conta" />
        <activity
            android:name=".editaritem.EditarItemActivity"
            android:label="SAF - Editar item" />
        <activity
            android:name=".configuracoes.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.edu.ifam.saf.MainActivity" />
        </activity>
    </application>

</manifest>